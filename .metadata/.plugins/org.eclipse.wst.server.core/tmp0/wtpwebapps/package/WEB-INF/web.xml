<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

           <servlet>
    	<servlet-name>PackLoginServlet</servlet-name>
    	<servlet-class>com.packages.PackLoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>PackLoginServlet</servlet-name>
    	<url-pattern>/log</url-pattern>
    </servlet-mapping>



          <servlet>
    	<servlet-name>packageServlet</servlet-name>
    	<servlet-class>com.packages.packageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>packageServlet</servlet-name>
    	<url-pattern>/insert</url-pattern>
    </servlet-mapping>



      <servlet>
    	<servlet-name>updatePack</servlet-name>
    	<servlet-class>com.packages.updatePackageSrevlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>updatePack</servlet-name>
    	<url-pattern>/update</url-pattern>
    </servlet-mapping>
  
  
  
  
  
   <servlet>
    	<servlet-name>deletePackageServlet</servlet-name>
    	<servlet-class>com.packages.deletePackageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>deletePackageServlet</servlet-name>
    	<url-pattern>/delete</url-pattern>
    </servlet-mapping>






</web-app>